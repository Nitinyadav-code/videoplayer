{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/data/*.json", "/sitemap.xml", "/*.ico"]
  },
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "headers": {
        "cache-control": "must-revalidate, max-age=3600"
      }
    },
    {
      "route": "/about",
      "serve": "/about.html"
    },
    {
      "route": "/videos",
      "serve": "/videos.html"
    },
    {
      "route": "/contact",
      "serve": "/contact.html"
    },
    {
      "route": "/video-player",
      "serve": "/video-player.html",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "route": "/video-player/*",
      "serve": "/video-player.html",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/videocards",
      "serve": "/videocards.html",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "route": "/videoplayer1",
      "serve": "/videoplayer1.html",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "route": "/videoplayer1/*",
      "serve": "/videoplayer1.html",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/sitemap.xml",
      "serve": "/sitemap.xml",
      "headers": {
        "content-type": "application/xml",
        "cache-control": "must-revalidate, max-age=86400"
      }
    },
    {
      "route": "/gallery",
      "serve": "/gallery.html"
    },
    {
      "route": "/termsandconditions",
      "serve": "/termsandconditions.html"
    },
    {
      "route": "/privacypolicy",
      "serve": "/privacypolicy.html"
    },
    {
      "route": "/data/*",
      "headers": {
        "cache-control": "no-cache",
        "content-type": "application/json"
      }
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https: data: blob: 'unsafe-eval' 'unsafe-inline'; object-src 'none'",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, OPTIONS"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".xml": "application/xml",
    ".html": "text/html"
  }
}
