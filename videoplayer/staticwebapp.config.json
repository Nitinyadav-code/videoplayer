{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/data/*", "/sitemap.xml"]
  },
  "routes": [
    {
      "route": "/about",
      "serve": "/about.html"
    },
    {
      "route": "/videos",
      "serve": "/videos.html"
    },
    {
      "route": "/contact",
      "serve": "/contact.html"
    },
    {
      "route": "/video-player",
      "serve": "/video-player.html"
    },
    {
      "route": "/data/*",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "route": "/sitemap.xml",
      "serve": "/sitemap.xml",
      "headers": {
        "content-type": "application/xml",
        "cache-control": "must-revalidate, max-age=86400"
      }
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  }
}
